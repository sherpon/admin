{
  "name": "admin-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "./scripts/development.build.sh",
    "build-staging": "./scripts/staging.build.sh",
    "build-production": "./scripts/production.build.sh",
    "watch": "webpack --watch --mode development --env.ENVIRONMENT=development --env.ENV_FILE_PATH=$PWD/env/development.env --env.FIREBASE_CONFIG_PATH=$PWD/env/development.firebaseConfig.json --env.PLANS_JSON_PATH=$PWD/env/development.plans.json",
    "start": "webpack-dev-server --mode development --env.ENVIRONMENT=development --env.ENV_FILE_PATH=$PWD/env/development.env --env.FIREBASE_CONFIG_PATH=$PWD/env/development.firebaseConfig.json --env.PLANS_JSON_PATH=$PWD/env/development.plans.json",
    "deploy-staging": "firebase deploy --only hosting:admin -P staging",
    "deploy-staging-jenkins": "firebase deploy --only hosting:admin -P staging --token $FIREBASE_DEPLOY_TOKEN",
    "deploy-production": "firebase deploy --only hosting:admin -P production",
    "deploy-production-jenkins": "firebase deploy --only hosting:admin -P production --token $FIREBASE_DEPLOY_TOKEN"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "author": "Bruce Grover Lee",
  "license": "GPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx-img-import": "^0.1.4",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "file-loader": "^3.0.1",
    "history": "^4.9.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.1",
    "jest": "^24.8.0",
    "jest-localstorage-mock": "^2.4.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "dotenv-webpack": "^1.7.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "brace": "^0.11.1",
    "grapesjs": "^0.14.62",
    "grapesjs-preset-webpage": "^0.1.11",
    "js-beautify": "^1.10.0",
    "prettysize": "^2.0.0",
    "react-ace": "^7.0.2",
    "react-dropzone": "^10.1.5"
  }
}
